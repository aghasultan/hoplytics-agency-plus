{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "hoplytics/roi-calculator",
	"version": "2.0.0",
	"title": "ROI Calculator",
	"category": "widgets",
	"icon": "calculator",
	"description": "Interactive ROI calculator powered by the WordPress Interactivity API.",
	"supports": {
		"interactivity": true,
		"align": [
			"wide",
			"full"
		],
		"color": {
			"background": true,
			"text": true
		},
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"textdomain": "hoplytics",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.js"
}